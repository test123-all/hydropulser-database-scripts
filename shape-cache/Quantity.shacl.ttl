@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix kraken: <https://aims-projekt.de/ap/fst/kraken/> .

kraken:Quantity
    dcterms:created "2022-11-23"^^xsd:date ;
	dcterms:creator "Nils Preu√ü" ;
	dcterms:description "a physical quantity with a quantitykind or unit"@en ;
	dcterms:rights "https://spdx.org/licenses/CC0-1.0.html" ;
	dcterms:subject "https://github.com/tibonto/dfgfo/4"@en ;
	dcterms:title "quantity"@en ;
	a sh:Nodeshape ;
	sh:closed false ;
	sh:property [
		sh:name "quantity kind"@en ;
		sh:order 7 ;
		sh:path qudt:hasQuantityKind ;
		sh:nodeKind sh:IRI ;
	], [
		sh:datatype xsd:string ;
		sh:name "symbol or variable name"@en ;
		sh:order 6 ;
		sh:path qudt:symbol ;
	], [
		sh:name "unit"@en ;
		sh:order 8 ;
		sh:path qudt:unit ;
		sh:nodeKind sh:IRI ;
	], [
		sh:name "value"@en ;
		sh:order 9 ;
		sh:path qudt:value ;
		sh:nodeKind sh:IRIOrLiteral ;
	] ;
	owl:imports kraken:Property ;
	sh:node kraken:Property .
